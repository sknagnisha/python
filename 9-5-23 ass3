**************ASSIGNMENT-3**************
import math
t=int(input())
for i in range(1,t+1):
    n=int(input())
    l=[]
    while(n%2==0):
         l.append(2)
         n=n//2
    for i in range(3,int(math.sqrt(n)+1),2):
         while(n%i==0):
            l.append(i)
             n=n//i
    if n>2:
        l.append(n)
    print(sum(l))
