#prgm for leap year by using class

class Solution:
    def leap(self,n):
        if n%4==0 and  n%400!=0:
            return True
        else:
            return False
        return True
n=int(input())
s=Solution()
if s.leap(n):
    print('yes')
else:
    print('no')
